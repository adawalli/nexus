{
  "name": "openrouter-search-mcp",
  "version": "1.0.0",
  "description": "MCP server for OpenRouter model search and discovery",
  "main": "dist/index.js",
  "bin": {
    "openrouter-search-mcp": "dist/cli.js"
  },
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run clean && tsc && npm run bundle && npm run fix-cli-shebang",
    "bundle": "esbuild dist/src/index.js --bundle --platform=node --target=node16 --format=esm --packages=external --outfile=dist/index.js && esbuild dist/src/cli.js --bundle --platform=node --target=node16 --format=esm --packages=external --outfile=dist/cli.js",
    "fix-cli-shebang": "echo '#!/usr/bin/env node' > dist/cli.tmp && cat dist/cli.js >> dist/cli.tmp && mv dist/cli.tmp dist/cli.js && chmod +x dist/cli.js",
    "dev": "nodemon --exec ts-node --esm src/index.ts",
    "start": "node dist/index.js",
    "cli": "node dist/cli.js",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:integration": "vitest run tests/integration",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "verify": "node scripts/build-verification.js",
    "prepublishOnly": "npm run lint && npm run format:check && npm run type-check && npm test && npm run build"
  },
  "keywords": [
    "mcp",
    "openrouter",
    "ai",
    "models"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.13.0",
    "axios": "^1.10.0",
    "data-masking": "^1.0.3",
    "dotenv": "^16.5.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/node": "^24.0.3",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "esbuild": "^0.25.5",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "msw": "^2.10.2",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
